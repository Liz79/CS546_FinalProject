<div class="main-page">
    <h2> Groceries </h2>
</div>
<div class="view-items">
    <ul class="product_list">
        {{#each products}}
        <li><a href="/products/{{this._id}}">{{this.name}}</a> - {{this.price}}<br />
            {{this.description}}<br />
            <img src={{this.pic}} height="90" width="120">
        </li>
        {{else}}
        <p>No items match your criteria.</p>
        {{/each}}
        {{!-- <li>items here</li> --}}
    </ul>
</div>
{{#if status}}
<p>status: {{status}} | userInfo:{{userInfo}}</p>
{{/if}}